--color_gem_skin = math.random(0, 14)
color_gem_skin = 0
color_gem_skin_count = 7
local function gem_sprite(gem)
	local base = 256 + color_gem_skin * 16
	local colour = gem & gem_colour_mask
	if colour >= gem_punch and colour <= gem_rainbow then
		local supercharge = ((gem & gem_supercharge) ~= 0) and 8 or 0
		return sprites_color_gem[colour] - 1 + base + supercharge
	else
		return sprites_other_gem[gem]
	end
end


local function scaled_gem(sprite, x, y, size)
	local fullsize <const> = 29
	local w, h = flr(fullsize * size + 0.5) // 2 * 2 + 1, flr(fullsize * size + 0.5) // 2 * 2 + 1
	local x, y = x + ((fullsize - w) / 2), y + ((fullsize - h) / 2)
	sspr(sprite, 0, 0, fullsize, fullsize, x, y, w, h)
end


local function draw_exploded_gem(colour, x, y, clock, new_clock)
	local size = 32 * new_clock
	local size2 = (32 + new_clock * 48) * new_clock + 6
	
	local colour = 15 + (colour - 1) * 5 + 1
	if clock < 0.25 then
		colour = 7
	end

	ovalfill(x + 3 - size, y + 3 - size, x + 26 + size, y + 26 + size, colour)
	oval(x + 3 - size2, y + 3 - size2, x + 26 + size2, y + 26 + size2, colour)
end


function gfx_draw_gems(cells, gems, marks, clock)
	local clock2 = clock * clock
	local clock4 = clock2 * clock2
	local function draw_gem(p, gem)
		local x, y = gems:coords_gem(p)
		local sprite = gem_sprite(gem)
		local cell = cells:get(p)
		if cell and cell & cell_mask == cell_bad then
			pal(53, 58)
		end
		local mark = marks:get(p)
		if mark and mark.logic_state == "new" then
			scaled_gem(sprite, x, y, clock)
		elseif mark and mark.anim == "clearing" then
			-- Needs to be drawn above other gems
		elseif mark and mark.anim == "cleared" then
			-- Nothing to draw
		elseif mark and mark.logic_state == "falling" then
			local dest_x, dest_y = gems:coords_gem(mark.target)
			local ix, iy = x + clock2 * (dest_x - x), y + clock2 * (dest_y - y)
			spr(sprite, ix, iy)
		elseif mark and mark.logic_state == "swapping" then
			local dest_x, dest_y = gems:coords_gem(mark.target)
			local ix, iy = x + clock2 * (dest_x - x), y + clock2 * (dest_y - y)
			spr(sprite, ix, iy)
		elseif mark and mark.logic_state == "moving" then
			x, y = gems:coords_gem(mark.source, gem)
			local dest_x, dest_y = gems:coords_gem(mark.target)
			scaled_gem(gem_sprite(mark.target_gem), dest_x, dest_y, 1.0)
		else
			spr(sprite, x, y)
		end
		pal()	
	end
	gems:foreachpos(draw_gem)
	gems:foreachpos(
		function(p, gem)
			local x, y = gems:coords_gem(p)
			local sprite = gem_sprite(gem)
			local cell = cells:get(p)
			if cell and cell & cell_mask == cell_bad then
				pal(53, 58)
			end
			local mark = marks:get(p)
			if mark and mark.anim == "clearing" then
				draw_exploded_gem(mark.match_colour, x, y, clock, clock4)
			elseif mark and mark.logic_state == "moving" then
				x, y = gems:coords_gem(mark.source, gem)
				local dest_x, dest_y = gems:coords_gem(mark.target)
				local ix, iy = x + clock * (dest_x - x), y + clock * (dest_y - y)
				spr(gem_sprite(mark.moving_gem), ix, iy)
			end
			pal()
		end
	)
end


