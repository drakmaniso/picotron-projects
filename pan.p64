--- Combat Logic


function logic_new_fighter(kind)
	local self = {
		health = 10,
		max_health = 10,
		armor = 3,
		max_armor = 3,
		block = 0,
		dodge = 0,
		max_dodge = 9,
		kick = 0,
		punch = 0,
		combos = 0,
		max_combos = 10,
	}
	
	function self:start_new_turn()
		self.block = 0
		self.punch = 0
		self.kick = 0
	end
	
	function self:count_points(game, gems, match)
		local points = 0
		for p in all(match.positions) do
			local gem = gems:get(p)
			local is_supercharged = (gem & gem_supercharge) ~= 0
			points += is_supercharged and 2 or 1
		end
		if match.colour == gem_punch or match.colour == gem_rainbow then
			self.punch += points
		end
		if match.colour == gem_kick or match.colour == gem_rainbow then
			self.kick += points // 2
		end
		if match.colour == gem_block or match.colour == gem_rainbow then
			self.block += points
		end
		if match.colour == gem_dodge or match.colour == gem_rainbow then
			self.dodge = min(self.max_dodge, self.dodge + points)
		end
		if match.colour == gem_armor or match.colour == gem_rainbow then
			self.armor = min(self.max_armor, self.armor + (points // 2))
		end
		self.combos += max(0, #match.positions - 3)
		self.combos = min(self.combos, self.max_combos)
		game.in_cascade = true -- TODO: keep? delete?
	end
	
	function self:enact_points(other)
		local other_is_dodging = (other.dodge == other.max_dodge)
		if self.punch > 0 then
			if other.block <= self.punch then
				self.punch -= other.block
				other.block = 0
			else
				other.block -= self.punch
				self.punch = 0
			end
			if self.punch > 0 and other_is_dodging then
				self.punch = 0
				other.dodge = 0
			end
			if other.armor <= self.punch then
				self.punch -= other.armor
				other.armor = 0
			else
				other.armor -= self.punch
				self.punch = 0
			end
			other.health = max(0, other.health - self.punch)
			self.punch = 0
		end
		if self.kick > 0 then
			if self.kick > 0 and other_is_dodging then
				self.kick = 0
				other.dodge = 0
			end
			if other.armor <= self.kick then
				self.kick -= other.armor
				other.armor = 0
			else
				other.armor -= self.kick
				self.kick = 0
			end
			other.health = max(0, other.health - self.kick)
			self.kick = 0
		end
	end
	
	function self:stop_combat()
		self.block = 0
		self.dodge = 0
		self.punch = 0
		self.kick = 0
	end
	
	return self
end


function logic_new_enemy()
	local self = logic_new_fighter()
	
--	self.max_health = 15
--	self.health = self.max_health
	
	function self:make_intent(turn)
		self.intent = {}
		if turn % 4 == 0 then
			self.intent.block = 3
			self.intent.armor = 1
		elseif turn % 4 == 1 then
			self.intent.punch = 3
			self.intent.armor = 1
		elseif turn % 4 == 2 then
			self.intent.block = 3
			self.intent.dodge = 3
		elseif turn % 4 == 3 then
			self.intent.kick = 1
		end
	end

	function self:apply_intent()
		self.punch = self.intent.punch or 0
		self.kick = self.intent.kick or 0
		self.dodge = min(self.max_dodge, self.dodge + (self.intent.dodge or 0))
		self.block = self.intent.block or 0
		self.armor = min(self.max_armor, self.armor + (self.intent.armor or 0))
		self.intent = nil
	end
	
	return self
end



