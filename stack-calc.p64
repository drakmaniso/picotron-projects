--[[pod_format="raw",created="2026-01-12 10:16:01",modified="2026-01-15 17:25:50",revision=2254]]
undo_stack = nil


function init_undo_stack()
	undo_stack = create_undo_stack(
		function()
			local state = {
				stack = get_stack_state(),
			}
			return state
		end,
		function(state)
			if type(state) == "table" then
				if state.stack then
					set_stack_state(state.stack)
				end
			end
		end
	)
end

