undo_stack = nil


function init_undo_stack()
	undo_stack = create_undo_stack(
		function()
			local state = {
				stack = get_stack_state()
			}
			return state
		end,
		function(state)
			if type(state) == "table" and state.stack then
				set_stack_state(state.stack)
			end
		end
	)
end

