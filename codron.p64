----------------------------------------------------------------------------------------


function init()
	_head = create_gui()
end


----------------------------------------------------------------------------------------


function update()
	_head:update_all()
end


----------------------------------------------------------------------------------------


function draw()
	_head:draw_all()
end


----------------------------------------------------------------------------------------


return {
	head = head,
	init = init,
	draw = draw,
	update = update,
	editor = include "src/ui/editor.lua",
	sidebar = include "src/ui/sidebar.lua",
}


