--[[pod_format="raw",created="2025-03-16 10:06:23",modified="2025-12-14 12:10:28",revision=898]]
-- module lexing --------------------------------------------------------------------------


local Lexer = newtype {
	-- source: String
	-- current: Integer
}


local Token = newtype {
	-- tag: String
	-- start: Integer
	-- fin: Integer
}


--------------------------------------------------------------------------------------------


function Lexer.new(source)
	return Lexer {
		source = source,
		current = 1,
	}
end


------------------------------------------------------------------------------------------


local symbols


function Lexer:next()
	local function token(tag, start, fin)
		return Token { tag = tag, start = start, fin = fin }
	end
	self:skip_whitespace()
	local start <const> = self.current
	local char <const> = self:peek()
	if char == nil then
		return token("EOF", start, self.current)
	end
	if is_letter(char) then
		return token(self:consume_identifier(), start, self.current - 1)
	end
	if is_digit(char) then
		return token(self:consume_number(), start, self.current - 1)
	end
	local consume_symbols = symbols[char]
	if type(consume_symbols) == "function" then
		return token(consume_symbols(self), start, self.current - 1)
	end
	return token(self:consume_invalid_char(), start, self.current - 1)
end


---------------------------------------------------------------------------------------------


function Lexer:peek()
	if self.current > #self.source then
		return nil
	end
	return self.source:byte(self.current)
end


function Lexer:peek_next()
	if self.current + 1 > #self.source then
		return nil
	end
	return self.source:byte(self.current + 1)
end


function Lexer:take()
	if self.current > #self.source then
		return
	end
	self.current += 1
end


--------------------------------------------------------------------------------------------


function Lexer:skip_whitespace(s)
	while true do
		local char = self:peek()
		if char == 32 or char == 9 then -- " " or "\t"
			self:take()
		else
			return
		end
	end
end


----------------------------------------------------------------------------------------------


local keywords = {
	"and", "break", "do", "else", "elseif", "end",
	"false", "for", "function", "goto", "if", "in",
	"local", "nil", "not", "or", "repeat", "return",
	"then", "true", "until", "while",
}


local keywords_tokens = {}
for keyword in all(keywords) do
	add(keywords_tokens, keyword:upper())
end


function Lexer:consume_identifier()
	local start = self.current
	self:take()
	while is_letter_or_digit(self:peek()) do
		self:take()
	end
	for keyword_index, keyword in ipairs(keywords) do
		if self.current - start == #keyword then
			local ps, pk = start, 1
			while pk <= #keyword and self.source:byte(ps) == keyword:byte(pk) do
				ps += 1
				pk += 1
			end
			if pk == #keyword + 1 then
				return keywords_tokens[keyword_index]
			end
		end
	end
	
	return "ID"
end


----------------------------------------------------------------------------------------------


function Lexer:consume_number()
	self:take()
	while is_digit(self:peek()) do
		self:take()
	end
	return "NUMBER"
end


function Lexer:consume_comment()
	-- TODO: how to handle multi-line comments? "--[[ ... ]]--"
	-- maybe just an error if the first char after "--" is "["?
	while self:peek() ~= nil and self:peek() ~= 10 do
		self:take()
	end
	return "COMMENT"
end


function Lexer:consume_invalid_char()
	self:take()
	return "INVALID"
end


----------------------------------------------------------------------------------------------


symbols = {
	[ord("\n")] =
		function(s)
			s:take()
			return "\n"
		end,
	[ord(";")] = function(s) s:take(); return ";" end,
	[ord("=")] =
		function(s)
			s:take()
			if s:peek() ~= 61 then -- second "="
				return "=" -- Double EQUAL
			end
			s:take()
			return "=="
		end,
	[ord(",")] = function(s) s:take(); return "," end,
	[ord(":")] =
		function(s)
			s:take()
			if s:peek() ~= 58 then -- second ":"
				return ":"
			end
			s:take()
			return "::"
		end,
	[ord(".")] =
		function(s)
			s:take()
			if s:peek() ~= 46 then -- second "."
				return "."
			end
			s:take()
			if s:peek() ~= 46 then -- third
				return ".."
			end
			s:take()
			return "..."
		end,
	[ord("[")] = function(s) s:take(); return "[" end,
	[ord("]")] = function(s) s:take(); return "]" end,
	[ord("(")] = function(s) s:take(); return "(" end,
	[ord(")")] = function(s) s:take(); return ")" end,
	[ord("{")] = function(s) s:take(); return "{" end,
	[ord("}")] = function(s) s:take(); return "}" end,
	[ord("+")] =
		function(s)
			s:take()
			if s:peek() ~= 43 then -- second "+"
				if s:peek() ~= 61 then -- "="
					return "+"
				end
				s:take()
				return "+="
			end
			s:take()
			if s:peek() ~= 61 then -- "="
				s:take()
				return "+="
			end
			s:take()
			return "++="
		end,
	[ord("-")] =
		function(s)
			s:take()
			if s:peek() ~= 45 then -- second "-"
				if s:peek() ~= 61 then -- "="
					return "-"
				end
				s:take()
				return "-="
			end
			return s:consume_comment()
		end,
	[ord("*")] =
		function(s)
			s:take()
			if s:peek() ~= 61 then -- "="
				return "*"
			end
			s:take()
			return "*="
		end,
	[ord("/")] =
		function(s)
			s:take()
			if s:peek() ~= 47 then -- second "/"
				if s:peek() ~= 61 then -- "="
					return "/"
				end
				s:take()
				return "/="
			end
			s:take()
			if s:peek() ~= 61 then -- "="
				s:take()
				return "//"
			end
			s:take()
			return "//="
		end,
	[ord("^")] =
		function(s)
			s:take()
			if s:peek() ~= 61 then -- "="
				return "^"
			end
			s:take()
			return "^="
		end,
	[ord("%")] =
		function(s)
			s:take()
			if s:peek() ~= 61 then -- "="
				return "%"
			end
			s:take()
			return "%="
		end,
	[ord("&")] =
		function(s)
			s:take()
			if s:peek() ~= 61 then -- "="
				return "&"
			end
			s:take()
			return "&="
		end,
	[ord("~")] =
		function(s)
			s:take()
			if s:peek() ~= 61 then -- "="
				return "~"
			end
			s:take()
			return "~="
		end,
	[ord("|")] =
		function(s)
			s:take()
			if s:peek() ~= 61 then -- "="
				return "|"
			end
			s:take()
			return "|="
		end,
	[ord("<")] =
		function(s)
			s:take()
			if s:peek() ~= 60 then -- second "<"
				if s:peek() ~= 61 then -- "="
					return "<"
				end
				s:take()
				return "<="
			end
			s:take()
			if s:peek() ~= 61 then -- "="
				s:take()
				return "<<"
			end
			s:take()
			return "<<="
		end,
	[ord(">")] =
		function(s)
			s:take()
			if s:peek() ~= 62 then -- second ">"
				if s:peek() ~= 61 then -- "="
					return ">"
				end
				s:take()
				return ">="
			end
			s:take()
			if s:peek() ~= 61 then -- "="
				s:take()
				return ">>"
			end
			s:take()
			return ">>="
		end,
	[ord(".")] =
		function(s)
			s:take()
			if s:peek() ~= 46 then -- second "."
				s:take()
				return "."
			end
			s:take()
			if s:peek() ~= 61 then -- "="
				s:take()
				return ".."
			end
			s:take()
			return "..="
		end,
	[ord("#")] = function(s) s:take(); return "#" end,

}


--------------------------------------------------------------------------------------------


function is_letter(char)
	if not char then return false end
	return (char >= 97 and char <= 122) -- between "a" and "z"
		or (char >= 65 and char <= 90) -- between "A" and "Z"
		or (char == 95) -- "_"
end


function is_digit(char)
	return char and char >= 48 and char <= 57
end


function is_letter_or_digit(char)
	return is_letter(char) or is_digit(char)
end


---------------------------------------------------------------------------------------------


function Lexer:print(token)
	if token.tag == "NL" then
		print("NL")
	elseif token.tag == "ID" or token.tag == "NUMBER" or token.tag == "COMMENT" then
		print(	token.tag .. "(" .. self.source:sub(token.start, token.fin) .. ") \0")
	else
		print(token.tag .. " \0")
	end
end


---------------------------------------------------------------------------------------------


return {
	Lexer = Lexer,
}
