-- module parsing ------------------------------------------------------------------------


local ast = require "src/ast.lua"
local lexing = require "src/lexing.lua"


local Parser = newtype {
	-- lexer: lexing.Lexer
	-- current: lexing.Token
	-- previous: lexing.Token
}


--------------------------------------------------------------------------------------------


function Parser.new(lexer)
	return Parser {
		lexer = lexer,
		current = lexer:next(),
		previous = nil,
	}
end


--------------------------------------------------------------------------------------------


function Parser:expression()
	return self:comparison()
end


function Parser:comparison()
	local expr = self:term()
	while self:match("==", "~=", "<", ">", "<=", ">=") do
		local operator = self.previous
		local right = self:term()
		expr = ast.BinaryExpr { left = expr, operator = operator, right = right }
 	end
end


function Parser:term()
	local expr = self:factor()
	while self:match("+", "-") do
		local operator = self.previous
		local right = self:factor()
		expr = ast.BinaryExpr { left = expr, operator = operator, right = right }
 	end
end


function Parser:factor()
	local expr = self:unary()
	while self:match("*", "/") do
		local operator = self.previous
		local right = self:unary()
		expr = ast.BinaryExpr { left = expr, operator = operator, right = right }
 	end
end


function Parser:unary()
	if self:match("-", "not") then
		local operator = self.previous
		local right = self:primary()
		expr = ast.UnaryExpr { operator = operator, right = right }
 	end
end


function Parser:primary()
	if self:match("-", "not") then
		local operator = self.previous
		local right = self:primary()
		expr = ast.UnaryExpr { operator = operator, right = right }
 	end
end


--------------------------------------------------------------------------------------------


function Parser:match(...)
	for tag in all {...} do
		if self:check(tag) then
			self:take()
			return true
		end
	end
	return false
end


function Parser:check(tag)
	return self.current.tag == tag
end


function Parser:take()
	if self.current.tag == "EOF" then return end
	self.previous = self.current
	self.current = self.lexer:next()
end


---------------------------------------------------------------------------------------------


return {
	Parser = Parser,
}
